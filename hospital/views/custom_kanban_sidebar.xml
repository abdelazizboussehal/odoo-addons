<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="hospital.id_custom_kanban_sidebar" model="ir.ui.view">
            <field name="name">Custom Kanban Sidebar</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.res_partner_kanban_view"/>
            <field name="arch" type="xml">
                <xpath expr="//kanban" position="attributes">
                    <attribute name="js_class">hospital.custom_kanban_sidebar_id</attribute>
                </xpath>

            </field>
        </record>
    </data>
</odoo>